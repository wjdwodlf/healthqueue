# Copy this file to /home/ubuntu/healthqueue/deploy/env and fill values.
# Do NOT commit secrets. This is only an example.

# Django
DJANGO_SETTINGS_MODULE=backend.settings
# SECRET_KEY is set on the production server's /home/ubuntu/healthqueue/.env
# Do NOT put actual secret values in this file or commit them to Git.

# Project paths
PROJECT_DIR=/home/ubuntu/healthqueue
VENV_PATH=/home/ubuntu/healthqueue/venv
PYTHON_BIN=${VENV_PATH}/bin/python
PIP_BIN=${VENV_PATH}/bin/pip
CELERY_BIN=${VENV_PATH}/bin/celery

# Broker
CELERY_BROKER_URL=redis://localhost:6379/0
# If you use Redis with password: redis://:password@host:6379/0

# Misc
DJANGO_ENV=production
CELERY_LOG_LEVEL=info

# Optional: user that will run the services
SERVICE_USER=ubuntu
SERVICE_GROUP=ubuntu
